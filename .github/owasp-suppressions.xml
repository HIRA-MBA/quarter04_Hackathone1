    <!-- Suppress development-only dependencies -->
    <suppress>
        <notes>
            <![CDATA[
            Development dependencies not included in production builds
            ]]>
        </notes>
        <filePath regex="true">.*[/\\]node_modules[/\\]@types[/\\].*</filePath>
        <cpe>cpe:/a:*:*</cpe>
    </suppress>

