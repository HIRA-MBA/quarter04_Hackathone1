{
  "name": "physical-ai-textbook",
  "version": "0.1.0",
  "private": true,
  "description": "Physical AI & Humanoid Robotics: Embodied Intelligence Textbook",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "lint": "eslint src/ --ext .ts,.tsx",
    "lint:fix": "eslint src/ --ext .ts,.tsx --fix",
    "test": "echo \"No tests configured yet\" && exit 0",
    "validate:content": "node scripts/validate-content.js",
    "validate:links": "node scripts/validate-links.js",
    "audit:accessibility": "node scripts/audit-accessibility.js",
    "optimize:images": "node scripts/optimize-images.js",
    "validate:all": "npm run validate:content && npm run validate:links && npm run audit:accessibility",
    "optimize:images:analyze": "node scripts/optimize-images.js",
    "optimize:images:fix": "node scripts/optimize-images.js --fix",
    "optimize:images:webp": "node scripts/optimize-images.js --webp"
  },
  "dependencies": {
    "@docusaurus/core": "3.9.2",
    "@docusaurus/preset-classic": "3.9.2",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.9.2",
    "@docusaurus/tsconfig": "3.9.2",
    "@docusaurus/types": "3.9.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "glob": "^10.3.10",
    "pa11y-ci": "^3.1.0",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0"
  }
}
